<div id="<%= fieldName %>-container" class="field">
<div class="fieldTitle"><%= fieldType.title %></div><%
if errors.has_key(fieldName):
    %>
<div class="fieldError"><%= errors[fieldName] %></div><%
%>
<input id="<%= fieldName %>-control" type="text" name="<%= fieldName %>" value="<%= content[fieldName] %>" />
</div>
